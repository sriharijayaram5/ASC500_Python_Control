            <Option Name="SelectRangeEndAddress"   Value="0x1501" />
            <Option Name="SelectRangeEndIndex"     Value="3" />
            <Option Name="DisplaySelectButtons"    Value="1" />
          </Plugin>
        </View>
        
        <View Dgroup="6"         X="3" Y="0" >
          <Plugin Type="Line Display"   Caption="Calibration Phase"  Size="50" >
            <Option Name="SelectPointAddress"      Value="0xC8" />
            <Option Name="SelectPointIndex"        Value="0" />
            <Option Name="SelectRangeStartAddress" Value="0x1500" />
            <Option Name="SelectRangeStartIndex"   Value="3" />
            <Option Name="SelectRangeEndAddress"   Value="0x1501" />
            <Option Name="SelectRangeEndIndex"     Value="3" />
            <Option Name="DisplaySelectButtons"    Value="1" />
          </Plugin>
        </View>
      </Tab>
      
      <Tab Name="Line View" >
        <View Channel="10"   Caption="Line View"          X="0" Y="0" >
          <Plugin Type="Line Display"  Caption="Line View" Size="50" >
            <Option Name="DisplayXRangeButtons" Value="1" />
            <Option Name="DisplaySelectButtons" Value="0" />
            <Option Name="SynthIndex" Value="1" />
          </Plugin>
        </View>
      </Tab>
      
      <Tab Name="Frequency Analysis" >
        <View Dgroup="8"  Caption="FFT"            X="0" Y="0" Caption="Frequency Analysis" >
          <Plugin Type="FFT Data Filter" Caption="FFT" >
            <Option Name="FftSizeMax" Value="65536" />
          </Plugin>
          <Plugin Type="Line Display"  Size="50" Caption="FFT Display"  >
            <Option Name="DisplayToolBar" Value="1" />
          </Plugin>
        </View>
      </Tab>
      
      <!--
         <Tab Name="Scroll View"  >
           <View Dgroup="7"    Caption="Scroll Display"        X="2" Y="0" >
             <Plugin Type="Scroll Display"  Size="50" />
           </View>
         </Tab>
         -->
      
      <Macro Type="Repeater" Count="3" >
        <Tab Name="Spec $+(Counter,1)$"  >
          <Macro Type="Include" File="spectroscopy.ngm" P0="$Counter$" P1="1" />
          <View Dgroup="$+(Counter,3)$"   X="1" Y="0" Height="6" >
            <Plugin Type="Line Display" Size="50" Caption="Spec $+(Counter,1)$" >
              <Option Name="DisplaySelectButtons" Value="0" />
            </Plugin>
          </View>
        </Tab>
      </Macro>
      
      <Tab Name="Soft Spec"  >
        <View Dgroup="9"    Caption="Soft Spectroscopy"     X="0" Y="1" >
          <Plugin Type="Line Display" Size="50" Caption="Soft Spectroscopy" >
            <Option Name="DisplaySelectButtons" Value="0" />
          </Plugin>
        </View>
      </Tab>
      
      <Tab Name="CL Contouring Error" Condition="==(ClLoop,1)" >
        <Widget Type="Box" Style="Flat"                       X="1" Y="0"  Width="10" />
        <View Caption="Scan Correlation View" ChannelSelector="Restricted" Dgroup="13"  X="0" Y="1"  Width="12" Height="10">
          <Plugin Type="Correlation Filter">
            <Option Name="contouringerroradr" Value="751"/>
            <Option Name="contouringerrorindex" Value="0"/>
          </Plugin>
          <Plugin Caption="Scan_Correlation_View" Type="Line Display">
            <Option Name="DisplayToolBar" Value="0" />
            <Option Name="displayselectbuttons" Value="0" />
          </Plugin>
        </View>
      </Tab>
      
    </Widget> <!-- Tabs -->
    
  </Widget> <!-- Box -->
    
</Panel>
